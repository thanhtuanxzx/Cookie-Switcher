{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/groups/save",
      "dest": "/api/groups/save.js"
    },
    {
      "src": "/api/groups/removeProfile",
      "dest": "/api/groups/removeProfile.js"
    },
    {
      "src": "/api/groups/([^/]+)$",
      "dest": "/api/groups/getGroup.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}

